 /**
  * Format a number to a locale string, showing "-" for 0, null, undefined, or NaN
  * Handles string inputs from PostgreSQL numeric columns (Neon driver returns strings)
+ *
+ * Formatting rules:
+ * - Numbers >= 10: Format as #,### (no decimals)
+ * - Numbers < 10 with decimals: Format as ##.## (up to 2 decimal places)
+ * - Integers < 10: No decimals
  */
 export function formatNumber(value: number | string | null | undefined): string {
   const num = typeof value === 'string' ? parseFloat(value) : value;
   if (num == null || num === 0 || !Number.isFinite(num)) {
     return '';
   }
-  return num.toLocaleString();
+
+  const absNum = Math.abs(num);
+
+  // For numbers >= 10, format with commas and no decimals
+  if (absNum >= 10) {
+    return Math.round(num).toLocaleString('en-US', {
+      minimumFractionDigits: 0,
+      maximumFractionDigits: 0,
+    });
+  }
+
+  // For numbers < 10, check if it has decimals
+  const hasDecimals = num % 1 !== 0;
+  if (hasDecimals) {
+    // Format with up to 2 decimal places (trim trailing zeros)
+    return num.toLocaleString('en-US', {
+      minimumFractionDigits: 0,
+      maximumFractionDigits: 2,
+    });
+  }
+
+  // Integer < 10, no decimals needed
+  return num.toLocaleString('en-US', {
+    minimumFractionDigits: 0,
+    maximumFractionDigits: 0,
+  });
 }
 
